<?xml version="1.0" encoding="utf-8"?>
<carbon.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <carbon.widget.LinearLayout
        android:elevation="5dp"
        app:carbon_cornerRadius="5dp"
        android:layout_margin="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="vertical"
        android:paddingTop="10dp"
        android:paddingBottom="10dp">
        <carbon.widget.LinearLayout
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <carbon.widget.TextView
                android:id="@+id/fv_nametoc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:textStyle="bold"
                android:fontFamily="@font/lato_regular"
                android:textColor="@color/button"
                android:text="Kompentensi"
                />
            <carbon.widget.TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:textStyle="bold"
                android:fontFamily="@font/lato_regular"
                android:textColor="@color/button"
                android:text="  >  "
                />
            <carbon.widget.TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:textStyle="bold"
                android:id="@+id/fv_namescoretype"
                android:textColor="@color/colorPrimary"
                android:fontFamily="@font/lato_regular"
                android:text="Kompentensi"
                />
        </carbon.widget.LinearLayout>
        <carbon.widget.TextView
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14sp"
            android:id="@+id/fv_descanswers"
            android:textStyle="bold"
            android:fontFamily="@font/lato_regular"
            android:text="Kompentensi"
            />
    </carbon.widget.LinearLayout>

</carbon.widget.RelativeLayout>